{
    "compilerOptions": {
        "module": "es2015",
        "target": "es2017",
        "outDir": "dist",
        "sourceMap": true,
        "noImplicitAny": true,
        "noUnusedLocals": false,
        "jsx": "react",
        "baseUrl": ".",
        "paths": {
            "eosjs": ["node_modules/eosjs/dist/index.d.ts"]
        }
    },
    "include": [
        "src/**/*.ts",
        "node_modules/eosjs/**/*.d.ts"
    ]
}